					<font fontName="Times New Roman" size="10" isBold="true"/>
				</textElement>
				<text><![CDATA[Các điều kiện và điều khoản:]]></text>
			</staticText>
			<staticText>
				<reportElement uuid="a626c9d7-d9d4-417c-aa44-a3607701ef31" x="30" y="490" width="68" height="15"/>
