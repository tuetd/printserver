				<textElement markup="html">
					<font fontName="Times New Roman" size="12"/>
				</textElement>
				<textFieldExpression><![CDATA["Bên Thế chấp không được tiến hành hoặc bỏ qua không tiến hành bất kỳ hành động nào<br />mà có thể gây ra bất cứ thiệt hại nào cho Tài sản Thế chấp."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="dd/MM/yyyy" isBlankWhenNull="true">
				<reportElement uuid="682479c2-d0e8-4aca-b920-c80d467a5cf9" x="72" y="526" width="483" height="39" isPrintWhenDetailOverflows="true"/>
				<textElement markup="html">
					<font fontName="Times New Roman" size="12"/>
				</textElement>
				<textFieldExpression><![CDATA["Bên Thế chấp bảo dưỡng, bảo trì và giữ Tài sản Thế chấp và bất kỳ phần nào của Tài<br />sản Thế chấp để đảm bảo Tài sản Thế chấp luôn ở trong tình trạng sử dụng tốt."]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement uuid="8dbc609e-d101-478a-8750-c0af6a82101d" x="42" y="526" width="30" height="39"/>
				<textElement>
					<font fontName="Times New Roman" size="12" isBold="true"/>
