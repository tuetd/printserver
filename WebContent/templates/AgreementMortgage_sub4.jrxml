				<textElement>
					<font fontName="Times New Roman" size="12" isBold="true"/>
				</textElement>
				<text><![CDATA[4.8]]></text>
			</staticText>
			<textField isStretchWithOverflow="true" pattern="dd/MM/yyyy" isBlankWhenNull="true">
				<reportElement uuid="682479c2-d0e8-4aca-b920-c80d467a5cf9" x="72" y="573" width="483" height="51" isPrintWhenDetailOverflows="true"/>
				<textElement markup="html">
					<font fontName="Times New Roman" size="12"/>
					<paragraph lineSpacing="Single"/>
				</textElement>
				<textFieldExpression><![CDATA["Bên Thế Chấp khi tiến hành sửa chữa Tài sản Thế chấp phải được sự chấp thuận bằng <br />văn bản của Bên Nhận Thế Chấp trong trường hợp việc sửa chữa đó làm thay đổi bất kỳ<br />nội dung nào trong Giấy tờ Sở hữu."]]></textFieldExpression>
			</textField>
			<textField isStretchWithOverflow="true" pattern="dd/MM/yyyy" isBlankWhenNull="true">
				<reportElement uuid="682479c2-d0e8-4aca-b920-c80d467a5cf9" x="72" y="640" width="483" height="59" isPrintWhenDetailOverflows="true"/>
				<textElement markup="html">
					<font fontName="Times New Roman" size="12"/>
				</textElement>
				<textFieldExpression><![CDATA["Trong trường hợp Tài sản Thế chấp bị mất hoặc giảm sút giá trị, Bên Thế chấp có nghĩa<br />vụ phục hồi Tài sản Thế chấp và/hoặc bổ sung một tài sản thế chấp khác theo yêu cầu<br />của Bên Nhận Thế chấp để đảm bảo tuân thủ nghĩa vụ thế chấp theo quy định tại Hợp<br />đồng này."]]></textFieldExpression>
			</textField>
			<staticText>
				<reportElement uuid="8dbc609e-d101-478a-8750-c0af6a82101d" x="42" y="640" width="30" height="59"/>
