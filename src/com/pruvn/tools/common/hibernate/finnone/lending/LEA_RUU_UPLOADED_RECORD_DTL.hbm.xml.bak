		<property name="emiamt" type="big_decimal">
			<column name="EMIAMT" precision="16" />
		</property>
		<property name="lppamt" type="big_decimal">
			<column name="LPPAMT" precision="16" />
		</property>
		<property name="bccamt" type="big_decimal">
			<column name="BCCAMT" precision="16" />
		</property>
		<property name="excessamt" type="big_decimal">
