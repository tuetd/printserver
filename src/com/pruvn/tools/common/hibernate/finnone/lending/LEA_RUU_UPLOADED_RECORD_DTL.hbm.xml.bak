		<property name="othcharge2" type="java.lang.Integer">
			<column name="OTHCHARGE2" precision="8" scale="0" />
		</property>
		<property name="othamt2" type="big_decimal">
			<column name="OTHAMT2" precision="16" />
		</property>
		<property name="othcharge3" type="java.lang.Integer">
			<column name="OTHCHARGE3" precision="8" scale="0" />
		</property>
		<property name="othamt3" type="big_decimal">
			<column name="OTHAMT3" precision="16" />
		</property>
		<property name="othcharge4" type="java.lang.Integer">
			<column name="OTHCHARGE4" precision="8" scale="0" />
		</property>
		<property name="othamt4" type="big_decimal">
			<column name="OTHAMT4" precision="16" />
		</property>
		<property name="branchid" type="java.lang.Integer">
			<column name="BRANCHID" precision="8" scale="0" />
		</property>
		<property name="remarks" type="string">
			<column name="REMARKS" length="100" />
		</property>
		<property name="userid" type="string">
			<column name="USERID" length="8" />
		</property>
		<property name="receiptno" type="string">
			<column name="RECEIPTNO" length="20" />
		</property>
		<property name="receipt_channel" type="string">
			<column name="RECEIPT_CHANNEL" length="8" />
		</property>
		<property name="agencyid" type="java.lang.Integer">
			<column name="AGENCYID" precision="8" scale="0" />
		</property>
		<property name="old_receipt_num" type="string">
			<column name="OLD_RECEIPT_NUM" length="20" />
		</property>
	</class>
</hibernate-mapping>