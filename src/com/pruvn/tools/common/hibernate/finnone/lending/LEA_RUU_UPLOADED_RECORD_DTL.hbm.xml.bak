		<property name="upload_date" column="UPLOAD_DATE"></property>
		<property name="receipt_date" column="RECEIPT_DATE"></property>
		<property name="agreementno" column="AGREEMENTNO"></property>
		<property name="cheque_no" column="CHEQUE_NO"></property>
		<property name="bankacnum" column="BANKACNUM"></property>
		<property name="receipt_amt" column="RECEIPT_AMT"></property>

